\ProvidesFile{latex-tagging-status.ltx}[{{ site.time | date: "%Y-%m-%d" }}]

%% LaTeX Tagging Status

%% The data below is an internal representation of the tagging status from
%% https://latex3.github.io/tagging-project/tagging-status/full
%% The TeX commands and their arguments are internal to code in latex-lab
%% and are likely to change.
%% So it is not possible to use them directly.
%% Instead latex-lab will provide higher level interfaces to the data!

%% LaTeX Project
%% Licence LPPL

%% Code Status                 Meaning
%% ---- -----                  -------
%% 4   compatible             Works without any issues when tagging is enabled
%% 3   partially-compatible   Currently partially compatible, see comments column
%% 2   currently-incompatible Currently incompatible with the tagging code, may be updated eventually
%% 1   no-support             Incompatible with the tagging code, not expected to be supported
%% 0   unknown                Status is not known
%%


\def\@kernel@tagging@status#1#2#3{\expandafter\chardef\csname t@status@#1.#2\endcsname#3\relax}

{%  assign t-s = site.data.tagging-status %}
{%- assign lb = "{" -%}
{%- assign rb = "}" -%}

{%- for p in t-s -%}
\@kernel@tagging@status{{lb}}{{p.name}}{{rb}}{{lb}}
    {%-  if p.type == "package" -%}sty{%- endif -%}
    {%-  if p.type == "class" -%}cls{%- endif -%}
    {%-  if p.type == "library" -%}ltx{%- endif -%}
}{
    {%-  if p.status == "compatible" -%}4{%- endif -%}
    {%-  if p.status == "partially-compatible" -%}3{%- endif -%}
    {%-  if p.status == "currently-incompatible" -%}2{%- endif -%}
    {%-  if p.status == "no-support" -%}1{%- endif -%}
    {%-  if p.status == "unknown" -%}0{%- endif -%}
}
{% endfor %}
