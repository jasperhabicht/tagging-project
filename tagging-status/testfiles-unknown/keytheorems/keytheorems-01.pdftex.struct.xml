   </Caption>
   <text-unit xmlns="https://www.latex-project.org/ns/dflt"
      id="ID.0122"
      rolemaps-to="Part"
     >
    <text xmlns="https://www.latex-project.org/ns/dflt"
       id="ID.0123"
       xmlns:Layout="http://iso.org/pdf/ssn/Layout"
       Layout:TextAlign="Justify"
       rolemaps-to="P"
      >
     <?MarkedContent page="2" ?>Let
     <Formula xmlns="http://iso.org/pdf2/ssn"
        id="ID.0124"
        title="math"
        af="tag-AFfile29.tex"
        xmlns:Layout="http://iso.org/pdf/ssn/Layout"
        Layout:Placement="Inline"
       >
      <?MarkedContent page="2" ?>ds2=h(z)|dz|2
     </Formula>
     <?MarkedContent page="2" ?>be a Hermitian pseudo-metric on
     <Formula xmlns="http://iso.org/pdf2/ssn"
        id="ID.0125"
        title="math"
        af="tag-AFfile30.tex"
        xmlns:Layout="http://iso.org/pdf/ssn/Layout"
        Layout:Placement="Inline"
       >
      <?MarkedContent page="2" ?>Dr
     </Formula>
     <?MarkedContent page="2" ?>,
     <Formula xmlns="http://iso.org/pdf2/ssn"
        id="ID.0126"
        title="math"
        af="tag-AFfile31.tex"
        xmlns:Layout="http://iso.org/pdf/ssn/Layout"
        Layout:Placement="Inline"
       >
      <?MarkedContent page="2" ?>h∈C2(Dr)
     </Formula>
     <?MarkedContent page="2" ?>, with
     <Formula xmlns="http://iso.org/pdf2/ssn"
        id="ID.0127"
        title="math"
        af="tag-AFfile32.tex"
        xmlns:Layout="http://iso.org/pdf/ssn/Layout"
        Layout:Placement="Inline"
       >
      <?MarkedContent page="2" ?>ω
     </Formula>
     <?MarkedContent page="2" ?>the associated
     <Formula xmlns="http://iso.org/pdf2/ssn"
        id="ID.0128"
        title="math"
        af="tag-AFfile33.tex"
        xmlns:Layout="http://iso.org/pdf/ssn/Layout"
        Layout:Placement="Inline"
       >
      <?MarkedContent page="2" ?>(1,1)
     </Formula>
     <?MarkedContent page="2" ?>-form. If
     <Formula xmlns="http://iso.org/pdf2/ssn"
        id="ID.0129"
        title="math"
        af="tag-AFfile34.tex"
        xmlns:Layout="http://iso.org/pdf/ssn/Layout"
        Layout:Placement="Inline"
       >
      <?MarkedContent page="2" ?>Ricω≥ω
     </Formula>
     <?MarkedContent page="2" ?>on
     <Formula xmlns="http://iso.org/pdf2/ssn"
        id="ID.0130"
        title="math"
        af="tag-AFfile35.tex"
        xmlns:Layout="http://iso.org/pdf/ssn/Layout"
        Layout:Placement="Inline"
       >
      <?MarkedContent page="2" ?>Dr
     </Formula>
     <?MarkedContent page="2" ?>, then
     <Formula xmlns="http://iso.org/pdf2/ssn"
        id="ID.0131"
        title="math"
        af="tag-AFfile36.tex"
        xmlns:Layout="http://iso.org/pdf/ssn/Layout"
        Layout:Placement="Inline"
       >
      <?MarkedContent page="2" ?>ω≤ωr
     </Formula>
     <?MarkedContent page="2" ?>on all of
     <Formula xmlns="http://iso.org/pdf2/ssn"
        id="ID.0132"
        title="math"
        af="tag-AFfile37.tex"
        xmlns:Layout="http://iso.org/pdf/ssn/Layout"
        Layout:Placement="Inline"
       >
      <?MarkedContent page="2" ?>Dr
     </Formula>
     <?MarkedContent page="2" ?>(or equivalently,
     <Formula xmlns="http://iso.org/pdf2/ssn"
        id="ID.0133"
        title="math"
        af="tag-AFfile38.tex"
        xmlns:Layout="http://iso.org/pdf/ssn/Layout"
        Layout:Placement="Inline"
       >
      <?MarkedContent page="2" ?>ds2≤ds2r
     </Formula>
     <?MarkedContent page="2" ?>).
