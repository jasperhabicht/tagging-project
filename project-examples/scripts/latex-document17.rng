  <define name="Chapter-Title">
    <element>
      <name ns="">Chapter-Title</name>
      <attribute name="rolemaps-to">
        <value>H3</value>
      </attribute>
      <ref name="pdf2-attributes"/>
      <optional>
        <attribute name="title"/>
      </optional>
      <zeroOrMore>
        <choice>
          <text/>
          <ref name="Span"/>
        </choice>
      </zeroOrMore>
    </element>
